<div class="row">
  <div class="col-xs-12" style="height:80px;"></div>
<div class="col-md-4 col-md-offset-1">
    <div class="event"> <%= @event.event_date.strftime("%m/%d") %> </div>
    <h2><%= @event.name %></h2>
</div>

    <div class="eventdesc">
        <div id="event-<%= @event.id %>" class="eventdesc2">
          <h3> <%= @event.event_date.strftime("%I:%M%p") %> at <%= @event.location %> </h3> <br>
          <%= @event.description %>
        </div>
    </div>
  </div>
